    <div class="modal fade" id="ModalFaseAcao" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body">
                    <iframe onload="removeAcaoLoading()" src="about:blank" style="border: 0; display: none;" id="AcaoTipoFrame"></iframe>
                </div>
                <div class="modal-footer">
                </div>
            </div>
        </div>
    </div>



    <div class="modal fade" id="modalNovaAcao" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body">
                    <iframe style="border: 0; width: 100%; height: 500px;" id="NovaAcaoFrame"></iframe>
                </div>
                <div class="modal-footer">
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="ModalGeral" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body">
                </div>
                <div class="modal-footer">
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="ModalTrocarImg" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body">
                    <input type='file' class='' onchange='getBase64(this.files[0])' />
                    <input type="hidden" id="imgData" />
                    <button class="terminar-upload-summernote btn btn-raised btn-success">Ok</button>
                </div>
                <div class="modal-footer">
                </div>
            </div>
        </div>
    </div>

    <div class="feedback left RPChat">
        <div class="tooltips">
            <div class="btn-group dropup">
                <button type="button" style="background-color: black!important;" class="btn-bug-report btn btn-raised btn-primary dropdown-toggle btn-circle btn-lg" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fa fa-comments fa-2x" title="RPChat" data-toggle="tooltip"></i>
                </button>
                <ul class="dropdown-menu dropdown-menu-right dropdown-menu-form">
                    <li style="height: 100%;">
                        <div class="report" style="height: 100%;">
                            <style>
                                .iframe-usuarios-online {
                                    height: 100% !important;
                                }
                            </style>
                            <div class="col-md-12">
                                <select style="width: 100%" data-tipo="usuario" placeholder="Pesquisar Contatos" data-custom="false" data-min="3" id="USU_ID_CHAT" runat="server">
                                </select>
                                <script>
                                    jQuery(document).ready(function () {
                                        iniciarEngineChat();
                                    });

                                    $('#USU_ID_CHAT').on('select2:select', function (e) {                                       
                                        iniciarChat($(this).data("usuid"), $(this).val(),$(this).find('option:selected').text())
                                        $(this).val(null).trigger('change');
                                    });
                                </script>
                            </div>
                            <div class="wrapper-usuarios-online">

                            </div>

                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div id="achievement">
        <div class="circle rotate"></div>
        <div class="copy">
            <h4>Achievement Unlocked!</h4>
            <p>Here are some internet points!</p>
        </div>
    </div>
</body>
</html>
