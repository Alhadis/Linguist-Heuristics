<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="MasterAgenteOutbound.master.cs" Inherits="OCMOS.pages.MasterPages.MasterAgenteOutbound" %>

<%
    Response.Cache.SetCacheability(HttpCacheability.NoCache);
    Response.Cache.AppendCacheExtension("no-store, must-revalidate");
    Response.AppendHeader("Pragma", "no-cache");
    Response.AppendHeader("Expires", "0");
    Response.Headers.Remove("Cache-Control");
    Response.AppendHeader("Cache-Control", "no-cache, no-store, must-revalidate");
%>
<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml" lang="es-CO">
<head runat="server">
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="Página administrativa para procesos de gestión de encuestas Alpina" />
    <meta name="author" content="Outsourcing SA" />
    <meta name="country" content="COL" />
    <meta name="currency" content="$" />

    <meta http-equiv="Expires" content="0" />
    <meta http-equiv="Last-Modified" content="0" />
    <meta http-equiv="Cache-Control" content="no-cache, mustrevalidate" />
    <meta http-equiv="Pragma" content="no-cache" />

    <title>Agente - OCMOS</title>

    <link rel="shortcut icon" type="image/x-icon" href="<% = ResolveClientUrl("~/Images/favicon.ico") %>" />
    <link href="<% = ResolveClientUrl("~/Images/favicon.ico") %>" rel="apple-touch-icon" />

    <link href="<% = ResolveClientUrl("~/pages/MasterPages/ControlCarga.css") %>" rel="stylesheet" />
    <link href="<% = ResolveClientUrl("~/css/bootstrap.min.css") %>" rel="stylesheet" />

    <%-- Chosen CSS --%>
    <link href="<% = ResolveClientUrl("~/js/chosen/chosen.css") %>" rel="stylesheet" />
    <link href="<% = ResolveClientUrl("~/js/jquery/jquery-ui.css") %>" rel="stylesheet" />

    <%-- DataTables CSS --%>
    <link href="<% = ResolveClientUrl("~/js/datatables-plugins/dataTables.bootstrap.css") %>" rel="stylesheet" />

    <%-- DataTables Responsive CSS --%>
    <link href="<% = ResolveClientUrl("~/js/datatables-responsive/dataTables.responsive.css") %>" rel="stylesheet" />

    <link href="<% = ResolveClientUrl("~/pages/MasterPages/master.css") %>" rel="stylesheet" />
    <link href="<% = ResolveClientUrl("~/css/sb-admin.css") %>" rel="stylesheet" />

    <%--<link href="<% = ResolveClientUrl("~/css/plugins/morris.css") %>" rel="stylesheet" />--%>
    <link href="<% = ResolveClientUrl("~/font-awesome/css/font-awesome.css") %>" rel="stylesheet" />

    <%-- Datetime Picker --%>
    <link href="<% = ResolveClientUrl("~/css/plugins/bootstrap-datetimepicker/css/bootstrap-datetimepicker.css") %>" rel="stylesheet" />

    <script type="text/javascript">
        function nobackbutton() {
            window.location.hash = "no-back-button";
            window.location.hash = "Again-No-back-button" //chrome        
            window.onhashchange = function () { window.location.hash = "no-back-button"; }
        }
    </script>

    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body onload="Start_Contador(); nobackbutton();" onkeypress="Stop_Contador();" onclick="Stop_Contador();" style="background-color: white;">

    <span id="S_Up" class="ir-arriba fa fa-chevron-up" title="Ir arriba"></span>

    <div id="Carga" style="display: none; background: rgba(20,20,20,0.9); border: 1px solid #ffffff; color: #ffffff; border-radius: 5px; float: left; z-index: 1100; position: fixed;">
        <div class="cssload-whirlpool"></div>
        <div style="text-align: center; vertical-align: middle; height: 100%; margin-top: 30%;">
            <h4 id="InternalMessage1" style="color: #ffffff; margin-top: 60px;">Un momento por favor, estamos procesando tu solicitud...</h4>
            <h5 id="InternalMessage2" style="color: #ffffff;"></h5>
        </div>
    </div>

    <%-- Cambiar el ID a «wrapper» para agregar el panel del menú al lado izquierdo--%>
    <div id="wrappers">

        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <a class="navbar-brand" href="<% = ResolveClientUrl("~/pages/Private/Agent/GestionOutbound/GestionOutbound.aspx") %>" title="Ir a Gestión Outbound">OCMOS</a>
            </div>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i><span id="L_User"></span><b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li>
                           <a href="#" style="cursor:default;"> <i class="fa fa-fw fa-user"></i><span id="L_Name"></span></a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="#" onclick="Log_Out_Transaction('Salida');" title="Cerrar Sesión y volver al login"><i class="fa fa-fw fa-power-off"></i>Cerrar Sesión</a>
                        </li>
                    </ul>
                </li>
            </ul>

        </nav>

        <div id="page-wrapper">
            <form autocomplete="off" style="padding-bottom: 50px;">
                <asp:ContentPlaceHolder ID="body" runat="server">
                </asp:ContentPlaceHolder>
            </form>
            <%-- /.row --%>

            <div class="alert alert-warning alert-dismissable alertMessage" id="WarnigAlert">
                <button type="button" class="close" title="Cerrar mensaje" onclick="CloseAlert('W');">&times;</button>
                <p id="Mensaje_Warnig">
                    <strong>¡Atención!</strong><br />
                    Es muy importante que leas este mensaje de alerta.
                </p>
            </div>

            <div class="alert alert-danger alert-dismissable alertMessage" id="ErrorAlert">
                <button type="button" class="close" title="Cerrar mensaje" onclick="CloseAlert('E');">&times;</button>
                <p id="Mensaje_Error">
                    <strong>¡Cuidado!</strong><br />
                    Es muy importante que leas este mensaje de peligro.
                </p>
            </div>

            <div class="alert alert-success alert-dismissable alertMessage" id="SuccessAlert">
                <button type="button" class="close" title="Cerrar mensaje" onclick="CloseAlert('S');">&times;</button>
                <p id="Mensaje_Success">
                    <strong>¡Completado!</strong><br />
                    Es muy importante que leas este mensaje de acción completada.
                </p>
            </div>

            <div class="alert alert-info alert-dismissable alertMessage" id="InfoAlert">
                <button type="button" class="close" title="Cerrar mensaje" onclick="CloseAlert('I');">&times;</button>
                <p id="Mensaje_Info">
                    <strong>¡información!</strong><br />
                    Este es un mensaje de información.
                </p>
            </div>

        </div>
        <%-- /#page-wrapper --%>
        <div class="col-md-12" style="bottom: 0px; position: fixed; color: white; display: block; text-align: center; width: 100%; background-color: #222222; z-index: 3;">
            <strong id="F_Copyright">Copyright © <a href="http://www.outsourcing.com.co/" target="_blank" style="color: #72afd2;" title="Página Web Oficial de Outsourcing S.A.">Outsourcing S.A.</a></strong>
            <div>Versión <span id="F_AppVersion">1.0.0.1</span></div>

        </div>
        <%-- /#footer --%>
    </div>
    <!-- /#wrapper -->
    <script src="<% = ResolveClientUrl("~/js/jquery.js") %>"></script>
    <script src="<% = ResolveClientUrl("~/js/jquery/jquery-ui.js") %>"></script>
    <script src="<% = ResolveClientUrl("~/js/bootstrap.min.js") %>"></script>
    <%-- Chosen JavaScript --%>
    <script src="<% = ResolveClientUrl("~/js/chosen/chosen.jquery.js") %>"></script>
    <script src="<% = ResolveClientUrl("~/js/chosen/docsupport/prism.js") %>" charset="utf-8"></script>
    <script src="<% = ResolveClientUrl("~/js/chosen/docsupport/init.js") %>" charset="utf-8"></script>

    <%-- DataTables JavaScript --%>
    <script src="<% = ResolveClientUrl("~/js/datatables/js/jquery.dataTables.min.js") %>"></script>
    <script src="<% = ResolveClientUrl("~/js/datatables-plugins/dataTables.bootstrap.min.js") %>"></script>
    <script src="<% = ResolveClientUrl("~/js/datatables-responsive/dataTables.responsive.js") %>"></script>
    <script src="<% = ResolveClientUrl("~/pages/MasterPages/master.js") %>"></script>
    <%--<script src="<% = ResolveClientUrl("~/js/plugins/morris/raphael.min.js") %>"></script>--%>
    <%--<script src="<% = ResolveClientUrl("~/js/plugins/morris/morris.min.js") %>"></script>--%>
    <%--<script src="<% = ResolveClientUrl("~/js/plugins/morris/morris-data.js") %>"></script>--%>

    <%-- Datetime Picker Bootstrap --%>
    <script src="<% = ResolveClientUrl("~/js/bootstrap-datetimepicker/moment.min.js") %>" ></script>
    <script src="<% = ResolveClientUrl("~/js/bootstrap-datetimepicker/bootstrap-datetimepicker.js") %>" ></script>
    <script src="<% = ResolveClientUrl("~/js/bootstrap-datetimepicker/bootstrap-datetimepicker.es.js") %>" ></script>

    <asp:ContentPlaceHolder ID="scripts" runat="server">
    </asp:ContentPlaceHolder>
</body>
</html>
